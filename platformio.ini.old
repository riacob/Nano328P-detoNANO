[env:nanoatmega328new]
platform = atmelavr
board = nanoatmega328new
framework = arduino
lib_deps = 
	marcoschwartz/LiquidCrystal_I2C@^1.1.4
	nrf24/RF24@1.4.5
	bblanchon/ArduinoJson@^6.20.0
monitor_speed = 115200



